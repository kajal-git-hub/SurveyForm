<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TfEditTextClose">
        <attr name="tintColor" format="color" />
    </declare-styleable>
    <declare-styleable name="SearchableSpinner">
        <attr name="hintText" format="string" />
    </declare-styleable>
    <declare-styleable name="TfTextView">
        <attr name="isBold" format="boolean" />
    </declare-styleable>
    <declare-styleable name="MaskedEditText">
        <attr name="mask" format="string" />
        <attr name="placeholder" format="string" />
    </declare-styleable>
    <declare-styleable name="RangeSeekBar">
        <attr name="absoluteMinValue" format="integer|float" />
        <attr name="absoluteMaxValue" format="integer|float" />
        <attr name="singleThumb" format="boolean" />
    </declare-styleable>
</resources>